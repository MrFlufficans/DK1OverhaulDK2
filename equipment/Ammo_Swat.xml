<Equipment>
    <!--========================================================-->
    <!--                    KSG                                 -->
    <!--========================================================-->
    	<Ammo name="12GA00BUCK_KSG"
			tooltip="@ammo_12GA00BUCK_name"
			description="@ammo_12GA00BUCK_KSG_desc"
			img="data/models/weapons/attachments/m257_buckshot_ammo_ui.dds"
		>
		<MobilityModifiers moveSpeedModifierPercent="0" turnSpeedModifierPercent="0"/>

		<!-- defines base ROF and sound of weapon -->
		<Params roundsPerSecond="4.5"
				audibleSoundRadius="50"
				physicsImpactForce="3.0"
				damageType="0"
				silenced="0">

			<!-- the damage value is linearly interpolated in the given range, between min/max damage -->
			<Damage start="15" end="3" startDist="0" endDist="50"/>

			<!-- base value, can be modified by AttackType/Abilities/Doctrine/etc. -->
			<CriticalChancePercent start="10" end="5" startDist="0" endDist="25"/>

			<!-- i.e. divides distance range in (end - start) discrete intervals for AP -->
			<ArmorPenetration start="6" end="3" startDist="0" endDist="20"/>
		</Params>
	</Ammo>

    <!--========================================================-->
    <!--                      Five Seven                        -->
    <!--========================================================-->

    <Ammo name="5_7_28_Five_SeveN"
			tooltip="@ammo_5_7_FS_name"
			description="@ammo_5_7_FS_Desc"
			img="data/models/weapons/attachments/m1911fmj_ammo_ui.dds"
	>
		<MobilityModifiers moveSpeedModifierPercent="0" turnSpeedModifierPercent="0"/>

		<Params	roundsPerSecond="6"
				audibleSoundRadius="50"
				physicsImpactForce="4.0"
				damageType="0"
				silenced="0">
			<Damage start="23" end="19" startDist="0" endDist="100"/>
			<CriticalChancePercent start="35" end="29" startDist="0" endDist="100"/>
			<ArmorPenetration start="2" end="2" startDist="0" endDist="100"/>
		</Params>
	</Ammo>
</Equipment>